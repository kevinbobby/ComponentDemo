<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a01375594.historyModule">

    <application
        android:allowBackup="true"
        android:name=".HistoryApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/history_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/history_AppTheme">

        <activity android:name=".HistoryMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HistoryDetailActivity" />
    </application>

</manifest>